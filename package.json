{
	"name": "shaderlabformatter",
	"displayName": "ShaderLabFormatter",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"author": {
        "name": "LiteFeel"
    },
	"publisher": "litefeel",
	"homepage": "https://github.com/litefeel/ShaderLabFormatter/blob/master/README.md",
	"repository": {
        "type": "git",
        "url": "https://github.com/litefeel/ShaderLabFormatter"
    },
    "bugs": "https://github.com/litefeel/ShaderLabFormatter/issues",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:shaderlib"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "shaderlib",
				"aliases": [
					"ShaderLab"
				],
				"extensions": [
					".shader",
					".cginc",
					".glslinc",
					".compute",
					".cg",
					".hlsl"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}