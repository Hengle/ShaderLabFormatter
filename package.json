{
	"name": "shaderlabformatter",
	"displayName": "ShaderLabFormatter",
	"description": "An VSCode plugin for formatting of ShaderLab",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.32.0"
	},
	"author": {
		"name": "LiteFeel"
	},
	"publisher": "litefeel",
	"homepage": "https://github.com/litefeel/ShaderLabFormatter/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/litefeel/ShaderLabFormatter.git"
	},
	"bugs": "https://github.com/litefeel/ShaderLabFormatter/issues",
	"categories": [
		"Formatters","Other", "Programming Languages"
	],
	"keywords": ["formatter","shaderlab","unity"],
	"icon": "images/icon.png",
	"activationEvents": [
		"onLanguage:shaderlab"
	],
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}